generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Transaction {
  id          String   @id @default(cuid())
  date        String   // YYYY-MM-DD
  amount      Int      // JPY, expenses are NEGATIVE
  category    String
  account     String
  description String
  hash        String   @unique
  createdAt   DateTime @default(now())
}
